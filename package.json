{
  "name": "eris",
  "version": "0.18.0",
  "description": "A NodeJS Discord library",
  "main": "./index.js",
  "exports": {
    ".": [
      {
        "require": "./index.js",
        "import": "./esm.mjs"
      },
      "./index.js"
    ],
    "./*": "./*.js",
    "./esm": "./esm.mjs"
  },
  "typings": "./index.d.ts",
  "engines": {
    "node": ">=10.4.0"
  },
  "scripts": {
    "format": "eslint --fix lib examples \"*.{js,mjs,ts}\"",
    "lint": "eslint --max-warnings 0 lib examples \"*.{js,mjs,ts}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abalabahaha/eris.git"
  },
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "eris",
    "wrapper"
  ],
  "author": "abalabahaha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abalabahaha/eris/issues"
  },
  "homepage": "https://abal.moe/Eris/",
  "dependencies": {
    "ws": "^8.2.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@stylistic/eslint-plugin": "^2.6.4",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^16.11.7",
    "@types/ws": "^8.2.0",
    "eslint": "^9.9.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "globals": "^15.9.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.2.0"
  },
  "optionalDependencies": {
    "opusscript": "^0.0.8",
    "tweetnacl": "^1.0.3"
  },
  "browser": {
    "@discordjs/opus": false,
    "child_process": false,
    "dgram": false,
    "dns": false,
    "fs": false,
    "tls": false,
    "tweetnacl": false,
    "ws": false
  }
}
